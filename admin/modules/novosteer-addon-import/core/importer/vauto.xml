				<set_price_field type="droplist" required="true" validate="A:1:2000" title="Price Field" empty="true">
					<function module="novosteer-addon-import" function="getTableFields" />
					<relation id="Field">							
						<text>
							<f1 field="Field" />
						</text>
					</relation>
				</set_price_field>

			</fields>
		</box>
	</fields>


	<remove_fields>
